\tikzstyle{nd0}=[]
\tikzstyle{arc1}=[->]
\tikzstyle{nd1}=[]
\tikzstyle{arc2}=[->]
\tikzstyle{nd2}=[]
\tikzstyle{arc3}=[->]
\tikzstyle{nd3}=[]



\renewcommand{\TState}[2]{
  \foreach \proc in {#2} {
        \only<#1>{ \node[current process] (\proc) at (\proc.center) {}; }
  };
}



